// finding area of square
#include <stdio.h>
#define pi 3.1415
int main() {
    float Area;
    float a=0;
    printf("enter side\n");
    scanf("%f",&a);
    Area=a*a;
    printf("area is %f",Area);
    return 0;
} 
// enter side
// 5.25
// area is 27.562500

// enter side
// -5
// area is 25.000000
