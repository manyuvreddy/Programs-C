//given list of n students, find number of them having marks greater than 70
//input => n, nlines->roll no, name, marks

#include <iostream>
using namespace std;

struct student 
{
    char rollno[100];
    char name[100];
    int marks;
};

int main() 
{
    int t;
    cin >> t;
    while (t--)
     {
        int n;
        cin >> n;
        student *list = new student[n];
        //list = new student [n]
        for (int i = 0; i < n; i++) 
        {
            cin >> list[i].rollno;
            cin >>list[i].name;
            cin >> list[i].marks;
        }

        int count = 0;
        for (int i = 0; i < n; i++) 
        {
            if (list[i].marks >= 70)
                count++;
        }

        cout << count << endl;

        delete[] list;
    }
    return 0;
}